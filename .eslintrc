{
	"env": {
		"browser": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/recommended",
		"next",
		"plugin:jsx-a11y/recommended",
		"prettier",
		"plugin:unicorn/recommended"
	],
	"plugins": [
		"@typescript-eslint",
		"jsx-a11y",
		// "prettier",
		"react-compiler",
		"unicorn"
	],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": "esnext",
		"sourceType": "module",
		"project": "./tsconfig.json"
	}
}
